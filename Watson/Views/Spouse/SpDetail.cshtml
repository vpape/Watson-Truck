@{
    ViewBag.Title = "SpDetail";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Spouse Detail</title>
</head>
<body>
    <div class="form-horizontal">
        <hr />
        <button class="collapsible" id="SpDetail"><h4>Spouse Detail</h4></button>
        <div class="content">
            <dl class="dl-horizontal">
                <dt>
