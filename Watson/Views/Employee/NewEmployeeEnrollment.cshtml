                <div class="col-md-10">
                    <input type="text" id="JobTitle" class="form-control" name="JobTitle" value="" />
                </div>
            </div>
            <div class="form-group">
                <label for="EmpNumber" class="col-md-2 control-label">Employee Number:</label>
                <div class="col-md-10">
                    <input type="text" id="EmpNumber" class="form-control" name="SSN" value="" />
                </div>
            </div>
