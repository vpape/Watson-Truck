<body>
    <div class="form-horizontal">
        <hr />
        <div class="collapsible" id="FirstStep">Enrollment</div>
        <div id="divEmpEnrollment" class="content">
            <p></p>
            <p></p>
            <div class="form-group">
                <label class="col-md-2 control-label">Employee Role:</label>
                <div class="col-md-10">
                    <select id="EmployeeRole" class="form-control">
                        <option id="ManagerRole" value="Manager">Manger</option>
                        <option id="EmployeeRole" value="Employee">Employee</option>
                    </select>
                </div>
            </div>
            <div class="form-group">
                <label for="CurrentEmployer" class="col-md-2 control-label">Current Employer:</label>
                <div class="col-md-10">
                    <input type="text" id="CurrentEmployer" class="form-control" name="CurrentEmployer" value="" />
                </div>
            </div>
            <div class="form-group">
